					; Check case: If n and r are the same, then nCr is = 1.
 0000025D  3B CA			cmp		ecx, edx
 0000025F  74 27			je		ExitCombinations

 00000261				Calculate:
						; Calculate denominator (r!*(n-r)!)
						; First calculate r!
 00000261  52					push	edx
 00000262  E8 0000002A				call	factorial
 00000267  8B D8				mov		ebx, eax
						
						; Calculate (n-r)!
 00000269  8B 45 10				mov		eax, [ebp + 16]
 0000026C  2B 45 0C				sub		eax, [ebp + 12]
 0000026F  8B D0				mov		edx, eax
 00000271  52					push	edx
 00000272  E8 0000001A				call	factorial
						
						; Multiply r! by (n-r)!
 00000277  F7 E3				mul		ebx
 00000279  8B C8				mov		ecx, eax
						
						; Calculate numerator (n!)
 0000027B  8B 55 10				mov		edx, [ebp + 16]
 0000027E  52					push	edx
 0000027F  E8 0000000D				call	factorial
						
						; Divide numerator by denominator
